<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>PhoneServlet</servlet-name>
		<servlet-class>sp.phone.servlet.PhoneServlet</servlet-class>
		<load-on-startup>2</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>TimerServlet</servlet-name>
		<servlet-class>sp.phone.servlet.TimerServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>



	<servlet-mapping>
		<servlet-name>PhoneServlet</servlet-name>
		<url-pattern>/servlet/PhoneServlet</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>TimerServlet</servlet-name>
		<url-pattern>/servlet/TimerServlet</url-pattern>
	</servlet-mapping>





	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
